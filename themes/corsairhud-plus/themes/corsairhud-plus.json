    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#4A4A4B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#73C43A"
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#FF9F00"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#FF6B00"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#33D4FF"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.class", "entity.name.type", "support.class"],
            "settings": {
                "foreground": "#FFED4E"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#FFB84D"
            }
        },
        {
            "name": "Parameters",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#FFC66D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": ["constant", "constant.language", "constant.character"],
            "settings": {
                "foreground": "#FF9F00"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#E84A00"
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#FFC66D"
            }
        },
        {
            "name": "Tags",
            "scope": ["entity.name.tag", "punctuation.definition.tag"],
            "settings": {
                "foreground": "#FF6B00"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#FFED4E"
            }
        },
        {
            "name": "Property",
            "scope": ["variable.other.property", "support.type.property-name"],
            "settings": {
                "foreground": "#61B6FF"
            }
        },
        {
            "name": "JSON Keys",
            "scope": ["support.type.property-name.json"],
            "settings": {
                "foreground": "#33D4FF"
            }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#6FECFF"
            }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#8EFF3A"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF4D17"
            }
        },
        {
            "name": "Markup Heading",
            "scope": ["markup.heading", "entity.name.section"],
            "settings": {
                "foreground": "#FFED4E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Bold",
            "scope": ["markup.bold"],
            "settings": {
                "foreground": "#FF9F00",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Italic",
            "scope": ["markup.italic"],
            "settings": {
                "foreground": "#FFC66D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup Link",
            "scope": ["markup.underline.link"],
            "settings": {
                "foreground": "#33D4FF"
            }
        },
        {
            "name": "Markup Code",
            "scope": ["markup.inline.raw", "markup.fenced_code"],
            "settings": {
                "foreground": "#73C43A"
            }
        },
        {
            "name": "Diff Added",
            "scope": ["markup.inserted", "meta.diff.header.to-file"],
            "settings": {
                "foreground": "#8EFF3A"
            }
        },
        {
            "name": "Diff Deleted",
            "scope": ["markup.deleted", "meta.diff.header.from-file"],
            "settings": {
                "foreground": "#FF4D17"
            }
        }
    ]
}
